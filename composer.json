{
    "name": "jjsquady/laravel-chatapi",
    "description": "Laravel Wrapper for Chat-Api for WhatApp messaging.",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Gon√ßalves Junior",
            "email": "jorge.techinfo@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "ChatApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ],
            "providers": [
                "ChatApi\\ChatApiServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
